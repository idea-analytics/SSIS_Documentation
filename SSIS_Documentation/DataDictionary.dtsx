                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @CurrentServer VARCHAR(255)

SET @CurrentServer = @@SERVERNAME

DECLARE @Databases TABLE (
	[Database] varchar(255)
)

DECLARE @Tables TABLE (
	[Table] varchar(255),
	[IsView] VARCHAR(1)
)

DECLARE @Metadata TABLE (
	[ServerName] varchar(255),
	[DatabaseName] varchar(255),
	[DatabaseDescription] varchar(1500),
	[Schema] varchar(255),
	[TableName] varchar(255),
	[TableDescription] varchar(1500),
	[IsView] VARCHAR(1),
	[ColumnName] varchar(255),
	[ColumnDescription] varchar(1500),
	[Position] int,
	[DataType] varchar(255),
	[IsNullable] varchar(255),
	[Constraint] varchar(255)
)

INSERT INTO @Databases

SELECT [name]  
FROM [master].[dbo].[sysdatabases] 
WHERE [name] NOT IN ('master', 'tempdb', 'model', 'msdb','SSISDB','ReportServer','ReportServerTempDB','AMDB')

DECLARE @DatabaseDescription varchar(1500)

	WHILE(SELECT  COUNT(*) FROM @Databases ) &gt; 0
		BEGIN 	

		DECLARE @CurrentDatabase VARCHAR(255)

		SET @CurrentDatabase = (SELECT TOP 1 [Database] FROM @Databases)
		SET @DatabaseDescription = ''
		DECLARE @DatabaseDescriptions TABLE (
			[DatabaseDescription] varchar(1500)
		)
		INSERT INTO @DatabaseDescriptions EXECUTE('SELECT TOP 1 CAST([value] AS varchar(1500)) FROM ['+ @CurrentDatabase +'].[sys].[extended_properties] WHERE [class_desc] = ''DATABASE'' AND [name] = ''MS_Description''')
		SET @DatabaseDescription = (SELECT TOP 1 [DatabaseDescription] FROM @DatabaseDescriptions)
		DELETE FROM @DatabaseDescriptions
		SET @DatabaseDescription = CASE WHEN @DatabaseDescription IS NULL THEN '' ELSE @DatabaseDescription END
		INSERT INTO @Tables EXECUTE('SELECT [name], 0  FROM  ['+ @CurrentDatabase +'].[sys].[tables] UNION ALL SELECT [name], 1 FROM  ['+ @CurrentDatabase +'].[sys].[views]')

		WHILE(SELECT  COUNT(*) FROM @Tables ) &gt; 0
			BEGIN

			DECLARE @CurrentTable VARCHAR(255)
			DECLARE @IsView VARCHAR(1)

			SET @CurrentTable = (SELECT TOP 1 [Table] FROM @Tables)
			SET @IsView = (SELECT TOP 1 [IsView] FROM @Tables)

			DECLARE @MetadataQuery  AS NVARCHAR(MAX);
			SET @MetadataQuery = 'SELECT ''' + @CurrentServer + ''' AS [SERVER_NAME], '''+ @CurrentDatabase + '''  AS [DATABASE_NAME], ''' + @DatabaseDescription + ''' AS [DATABASE_DESCRIPTION], [SCHEMA], CAST(A.[TABLE_NAME] AS varchar) AS [TABLE_NAME], CAST([TABLE_DESCRIPTION] AS varchar(1500)) AS [TABLE_DESCRIPTION],  CAST( ''' + @IsView + ''' AS bit) AS [IsView], CAST(A.[COLUMN_NAME] AS varchar(255)) AS [COLUMN_NAME], CAST([COLUMN_DESCRIPTION] AS varchar(1500)) AS [COLUMN_DESCRIPTION], CAST(A.[ORDINAL_POSITION] AS INT) AS [ORDINAL_POSITION], CAST(A.[DATA_TYPE] AS varchar) AS [DATA_TYPE], CAST(A.[IS_NULLABLE] AS varchar) AS [IS_NULLABLE], CAST(B.[CONSTRAINT_NAME] AS varchar(255)) AS [CONSTRAINT_NAME] 
				FROM 
				(SELECT  [TABLE_NAME], [COLUMN_NAME], [ORDINAL_POSITION], [DATA_TYPE], [IS_NULLABLE] 
				FROM ['+ @CurrentDatabase +'].[INFORMATION_SCHEMA].[COLUMNS] 
				WHERE [TABLE_NAME] = ''' + @CurrentTable + ''') A 
				LEFT JOIN
				(SELECT [TABLE_NAME], [COLUMN_NAME],[CONSTRAINT_NAME]
					FROM ['+ @CurrentDatabase +'].[INFORMATION_SCHEMA].[KEY_COLUMN_USAGE] 
					WHERE [CONSTRAINT_NAME] IN ( SELECT [CONSTRAINT_NAME] FROM ['+ @CurrentDatabase +'].[INFORMATION_SCHEMA].[REFERENTIAL_CONSTRAINTS])
					AND [TABLE_NAME] = ''' + @CurrentTable + ''') B
				ON A.[COLUMN_NAME] = B.[COLUMN_NAME] AND  A.[TABLE_NAME] = B.[TABLE_NAME]
				LEFT JOIN
				(SELECT A.[NAME] AS [TABLE], B.[VALUE] AS [TABLE_DESCRIPTION], [schema_id]
					FROM ['+ @CurrentDatabase +'].[sys].[tables] A   
					LEFT JOIN ['+ @CurrentDatabase +'].[sys].[extended_properties] B 
					ON A.[OBJECT_ID] = B.[MAJOR_ID]
					AND B.[NAME] = ''MS_Description''
					AND B.[MINOR_ID] = 0 ) C
				ON C.[TABLE] = A.[TABLE_NAME]
				LEFT JOIN
				(SELECT A.[NAME] AS [TABLE], B.[NAME] AS [Column], C.[VALUE] AS [COLUMN_DESCRIPTION]
					FROM ['+ @CurrentDatabase +'].[sys].[tables] A
					INNER JOIN ['+ @CurrentDatabase +'].[sys].[columns] B 
					ON A.[OBJECT_ID] = B.[OBJECT_ID]
					INNER JOIN ['+ @CurrentDatabase +'].[sys].[extended_properties] C 
					ON A.[OBJECT_ID] = C.[MAJOR_ID]
					AND B.column_id = C.[MINOR_ID]
					AND C.[NAME] = ''MS_Description'' ) D
				ON D.[TABLE] = A.[TABLE_NAME] AND D.[Column] = A.[COLUMN_NAME]
				LEFT JOIN 
				(SELECT [NAME] AS [SCHEMA], [schema_id]
					FROM ['+ @CurrentDatabase +'].[sys].[schemas] ) E
					ON C.[schema_id] = E.[schema_id]'

			INSERT INTO @Metadata EXECUTE(@MetadataQuery)
			DELETE FROM @Tables WHERE [Table] = @CurrentTable
			END

		DELETE FROM @Databases WHERE [Database] = @CurrentDatabase
		END

		SELECT * FROM @Metadata</property>
