          SQLTask:Connection="{24952DD6-092F-40C0-8E10-5A037128F880}"
          SQLTask:SqlStatementSource="&#xA;--DECLARE @Servers TABLE (&#xA;--&#x9;[ID] bigint,&#xA;--&#x9;[ServerName] varchar(255)&#xA;--)&#xA;&#xA;&#xA;UPDATE [Documentation].[dbo].[Metadata]  SET [Schema] = 'dbo' WHERE [IsView] = 1&#xA;&#xA;&#xA;&#xA;--INSERT INTO [Documentation].[dbo].[Server] SELECT DISTINCT  A.[ServerName], '' FROM  [Documentation].[dbo].[Metadata] A &#xA;&#xA;--INSERT INTO @Servers SELECT DISTINCT [ID],  A.[ServerName] FROM [Documentation].[dbo].[Server]  A &#xA;&#xA;&#xA;--WHILE(SELECT  COUNT(*) FROM @Servers ) &gt; 0&#xA;--&#x9;&#x9;BEGIN &#x9;&#xA;&#x9;&#x9;&#xA;--&#x9;&#x9;DECLARE @CurrentServer VARCHAR(255)&#xA;--&#x9;&#x9;DECLARE @CurrentServerID bigint&#xA;--&#x9;&#x9;SET @CurrentServer = (SELECT TOP 1 [ServerName] FROM @Servers)&#xA;--&#x9;&#x9;SET @CurrentServerID = (SELECT TOP 1 [ID] FROM @Servers)&#xA;&#xA;--&#x9;&#x9;DECLARE @Databases TABLE (&#xA;--&#x9;&#x9;&#x9;&#x9;[DatabaseName] varchar(255)&#xA;--&#x9;&#x9;&#x9;)&#xA;--&#x9;&#x9;INSERT INTO @Databases SELECT  DISTINCT [DatabaseName] FROM [Documentation].[dbo].[Metadata] WHERE [ServerName] = @CurrentServer&#xA;&#xA;--&#x9;&#x9;WHILE(SELECT  COUNT(*) FROM @Databases ) &gt; 0&#xA;--&#x9;&#x9;&#x9;BEGIN&#xA;&#xA;--&#x9;&#x9;&#x9;DECLARE @CurrentDatabase VARCHAR(255)&#xA;--&#x9;&#x9;&#x9;DECLARE @CurrentDatabaseID bigint&#xA;--&#x9;&#x9;&#x9;SET @CurrentDatabase = (SELECT TOP 1 [DatabaseName] FROM @Databases)&#xA;&#x9;&#x9;&#x9;&#xA;--&#x9;&#x9;&#x9;INSERT INTO [Documentation].[dbo].[Database] SELECT DISTINCT @CurrentServerID, [DatabaseName],[DatabaseDescription] FROM [Documentation].[dbo].[Metadata] WHERE [ServerName] = @CurrentServer AND [DatabaseName] = @CurrentDatabase&#xA;--&#x9;&#x9;&#x9;SET @CurrentDatabaseID = @@IDENTITY;&#xA;&#xA;--&#x9;&#x9;&#x9;DECLARE @Tables TABLE (&#xA;--&#x9;&#x9;&#x9;&#x9;[TableName] varchar(255)&#xA;--&#x9;&#x9;&#x9;)&#xA;&#xA;--&#x9;&#x9;&#x9;&#x9;INSERT INTO @Tables SELECT DISTINCT [TableName] FROM [Documentation].[dbo].[Metadata] WHERE [ServerName] = @CurrentServer AND [DatabaseName] = @CurrentDatabase &#xA;&#x9;&#x9;&#x9;&#xA;&#xA;--&#x9;&#x9;&#x9;WHILE(SELECT  COUNT(*) FROM @Tables ) &gt; 0&#xA;--&#x9;&#x9;&#x9;&#x9;BEGIN&#xA;--&#x9;&#x9;&#x9;&#x9;DECLARE @CurrentTable VARCHAR(255)&#xA;--&#x9;&#x9;&#x9;&#x9;DECLARE @CurrentTableID bigint&#xA;--&#x9;&#x9;&#x9;&#x9;SET @CurrentTable = (SELECT TOP 1 [TableName] FROM @Tables)&#xA;&#xA;--&#x9;&#x9;&#x9;&#x9;INSERT INTO [Documentation].[dbo].[Table] SELECT DISTINCT @CurrentDatabaseID,[Schema], [TableName] ,[TableDescription], [IsView] FROM [Documentation].[dbo].[Metadata] WHERE [ServerName] = @CurrentServer AND [DatabaseName] = @CurrentDatabase AND [TableName] = @CurrentTable&#xA;--&#x9;&#x9;&#x9;&#x9;SET @CurrentTableID = @@IDENTITY;&#xA;&#x9;&#x9;&#x9;&#x9;&#xA;--&#x9;&#x9;&#x9;&#x9;INSERT INTO [Documentation].[dbo].[Column]  SELECT @CurrentTableID, [ColumnName],[ColumnDescription],[Position],[DataType],[IsNullable] ,[Constraint] FROM [Documentation].[dbo].[Metadata] WHERE [ServerName] = @CurrentServer AND [DatabaseName] = @CurrentDatabase AND [TableName] = @CurrentTable&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9; &#xA;--&#x9;&#x9;&#x9;&#x9;DELETE FROM @Tables WHERE [TableName] = @CurrentTable&#xA;--&#x9;&#x9;&#x9;&#x9;END&#xA;&#xA;--&#x9;&#x9;&#x9;DELETE FROM @Databases WHERE [DatabaseName] = @CurrentDatabase&#xA;--&#x9;&#x9;&#x9;END&#xA;&#xA;--&#x9;&#x9;DELETE FROM @Servers WHERE [ID] = @CurrentServerID&#xA;--&#x9;&#x9;END &#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
